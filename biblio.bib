@article{sloan1991,
 ISSN = {00255718, 10886842},
 URL = {http://www.jstor.org/stable/2008533},
 abstract = {Consider solving the Dirichlet problem $$\Delta u(P) = 0, P \in \mathbb R^2\backslash S,$$ $$u(P) = h(P),\quad P \in S,$$ $$\sup|u(P)| < \infty,$$ $$P \in \Bbb{R}^2$$ with $S$ a smooth open curve in the plane. We use single-layer potentials to construct a solution $u(P)$. This leads to the solution of equations of the form $$\int_S g(Q) \log|P - Q|dS(Q) = h(P), \quad P \in S.$$ This equation is reformulated using a special change of variable, leading to a new first-kind equation with a smooth solution function. This new equation is split into a principal part, which is explicitly invertible, and a compact perturbation. Then a discrete Galerkin method that takes special advantage of the splitting of the integral equation is used to solve the equation numerically. A complete convergence analysis is given; numerical examples conclude the paper.},
 author = {Kendall E. Atkinson and Ian H. Sloan},
 journal = {Mathematics of Computation},
 number = {193},
 pages = {119--139},
 publisher = {American Mathematical Society},
 title = {The Numerical Solution of First-Kind Logarithmic-Kernel Integral Equations on Smooth Open Arcs},
 volume = {56},
 year = {1991}
}

